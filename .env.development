# Development Environment
NODE_ENV=development
VITE_APP_NAME="NFT Nexus (Dev)"

# Solana Configuration - Devnet
VITE_SOLANA_NETWORK=devnet
VITE_SOLANA_RPC_URL=https://api.devnet.solana.com
# TODO: Replace with your actual program ID after deployment
VITE_PROGRAM_ID=YourProgramIdHere
VITE_MARKETPLACE_NAME="NFT-Nexus-Dev"

# Solana RPC Configuration
VITE_COMMITMENT=confirmed
VITE_PREFLIGHT_COMMITMENT=confirmed
VITE_SKIP_PREFLIGHT=false

# Transaction Configuration (more lenient for development)
VITE_DEFAULT_COMPUTE_UNIT_LIMIT=300000
VITE_DEFAULT_COMPUTE_UNIT_PRICE=1000
VITE_TRANSACTION_TIMEOUT=60000
VITE_MAX_RETRIES=5

# Development API
VITE_API_URL=http://localhost:8080/api

# NFT Metadata Configuration
VITE_METADATA_API_URL=http://localhost:8081
VITE_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs

# Analytics disabled in development
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_BETA_FEATURES=true
VITE_ENABLE_PRIORITY_FEES=true
VITE_ENABLE_TRANSACTION_RETRY=true

# Development Configuration
VITE_DEV_ENABLE_LOGS=true
VITE_DEV_MOCK_WALLET=false